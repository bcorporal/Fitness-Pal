<html lang='en'>
  <head>
    <meta charset='utf-8' />
    <link
      rel='stylesheet'
      href='https://cdn.jsdelivr.net/npm/fullcalendar@5.11.0/main.min.css'
    />
    <script
      src='https://cdn.jsdelivr.net/npm/fullcalendar@5.11.0/main.min.js'
    ></script>
    <script>

      document.addEventListener('DOMContentLoaded', function() { var calendarEl
      = document.getElementById('calendar'); var calendar = new
      FullCalendar.Calendar(calendarEl, { initialView: 'dayGridWeek',events: [
      
      {{#each workoutData}}
        {title:"{{this.name}}", 
        start:"{{this.date}}", 
        end:"{{this.date}}"},
      {{/each}}
      ] });
      calendar.render(); });

    </script>
  </head>
  <body>
    <div id='calendar'></div>
    {{#each workoutData}}

      {{/each}}
    <script type='text/javascript'>

      const calenderData =
      {{{json workoutData}}}; console.log(calenderData); 
    </script>
  </body>
</html>